{
	"manifest_version": 2,
	"name" : "Yourtube by petitcroco",
	"version" : "1.1",

	"description" : "ma description !! (a remplacer)",

	"icons" : {
		"128" : "logo.png"
	},

	"content_scripts" : [
		{
			"matches" : ["*://*.youtube.com/*"],
			"js" : ["background.js"]
		}
	],

	"browser_action" : {
		"default_icon" : "logo.png",
		"default_popup" : "popup.html"
	}
}